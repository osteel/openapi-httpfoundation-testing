{
    "name": "osteel/openapi-httpfoundation-testing",
    "type": "library",
    "description": "Validate HttpFoundation requests and responses against OpenAPI (3+) definitions",
    "keywords": [
        "openapi",
        "httpfoundation",
        "symfony",
        "laravel",
        "http",
        "validation",
        "testing",
        "api"
    ],
    "homepage": "https://github.com/osteel/openapi-httpfoundation-testing",
    "license": "MIT",
    "authors": [
        {
            "name": "Yannick Chenot",
            "email": "yannick@yellowraincoat.co.uk",
            "homepage": "https://github.com/osteel",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "league/openapi-psr7-validator": "^0.19",
        "nyholm/psr7": "^1.0",
        "psr/http-message": "^1.0",
        "symfony/http-foundation": "^4.0 || ^5.0 || ^6.0",
        "symfony/psr-http-message-bridge": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "rector/rector": "^0.17.1",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Osteel\\OpenApi\\Testing\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Osteel\\OpenApi\\Testing\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "fix": "phpcbf src tests",
        "refactor": "rector process"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
